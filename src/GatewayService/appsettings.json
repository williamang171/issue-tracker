{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "projects": {
        "ClusterId": "projectIssues",
        "Match": {
          "Path": "/api/projects/{**catch-all}",
          "Methods": [
            "GET",
            "POST",
            "PUT",
            "DELETE",
            "PATCH"
          ]
        }
      },
      "issues": {
        "ClusterId": "projectIssues",
        "Match": {
          "Path": "/api/issues/{**catch-all}",
          "Methods": [
            "GET",
            "POST",
            "PUT",
            "DELETE",
            "PATCH"
          ]
        }
      },
      "projectAssignments": {
        "ClusterId": "projectIssues",
        "Match": {
          "Path": "/api/projectAssignments/{**catch-all}",
          "Methods": [
            "GET",
            "POST",
            "PATCH",
            "PUT",
            "DELETE"
          ]
        }
      },
      "comments": {
        "ClusterId": "projectIssues",
        "Match": {
          "Path": "/api/comments/{**catch-all}",
          "Methods": [
            "GET",
            "POST",
            "PATCH",
            "PUT",
            "DELETE"
          ]
        }
      },
      "upload": {
        "ClusterId": "projectIssues",
        "Match": {
          "Path": "/api/upload/{**catch-all}",
          "Methods": [
            "GET",
            "POST",
            "PATCH",
            "PUT",
            "DELETE"
          ]
        }
      },
      "attachments": {
        "ClusterId": "projectIssues",
        "Match": {
          "Path": "/api/attachments/{**catch-all}",
          "Methods": [
            "GET",
            "POST",
            "PATCH",
            "PUT",
            "DELETE"
          ]
        }
      },
      "issueStats": {
        "ClusterId": "issueStats",
        "Match": {
          "Path": "/api/issueStats/{**catch-all}",
          "Methods": [
            "GET"
          ]
        }
      },
      "users": {
        "ClusterId": "users",
        "Match": {
          "Path": "/api/users/{**catch-all}",
          "Methods": [
            "GET",
            "POST",
            "PATCH",
            "PUT",
            "DELETE"
          ]
        }
      },
      "roles": {
        "ClusterId": "users",
        "Match": {
          "Path": "/api/roles/{**catch-all}",
          "Methods": [
            "GET",
            "POST",
            "PATCH",
            "PUT",
            "DELETE"
          ]
        }
      }
    }
  }
}